<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dim_5"
    android:background="@color/black"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/for_screenshot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <RelativeLayout
        android:id="@+id/rank_row_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_30"
        android:layout_alignParentTop="true"
        android:background="#0f2a48"
        android:orientation="vertical"
        android:visibility="gone">

        <com.jeeva.android.widgets.customfont.CustomTextView
            android:id="@+id/schedule_row_tv_leaderboard_rank_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dim_20"
            android:text="CURRENT RANK"
            android:textAllCaps="true"
            android:textColor="#a1afbe"
            android:textSize="@dimen/sp_10"
            app:typeface="lato_regular" />

        <com.jeeva.android.widgets.customfont.CustomTextView
            android:id="@+id/schedule_row_tv_leaderboard_rank"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="NO RANK"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_10"
            android:layout_toLeftOf="@+id/schedule_row_tv_leaderboard_total_players"
            app:typeface="lato_bold" />

        <com.jeeva.android.widgets.customfont.CustomTextView
            android:id="@+id/schedule_row_tv_leaderboard_total_players"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dim_20"
            android:textAllCaps="true"
            android:textColor="#a1afbe"
            android:textSize="@dimen/sp_10"
            android:layout_alignParentRight="true"
            app:typeface="lato_bold" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/match_details_with_leaderboard_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/rank_row_rl">

        <LinearLayout
            android:id="@+id/schedule_row_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/dim_10">

            <RelativeLayout
                android:id="@+id/schedule_row_rl_match_stage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/dp_2"
                android:background="@color/colorBackground"
                android:visibility="gone">

                <com.jeeva.android.widgets.customfont.CustomTextView
                    android:id="@+id/schedule_row_tv_match_stage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:padding="@dimen/dp_10"
                    android:textColor="@color/silver_chalice"
                    android:textSize="@dimen/dp_12" />


            </RelativeLayout>

            <!-- date -->
            <com.jeeva.android.widgets.customfont.CustomTextView
                android:id="@+id/schedule_row_tv_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="@color/grey6"
                android:textSize="@dimen/sp_10"
                app:typeface="lato_bold" />

            <RelativeLayout
                android:id="@+id/image_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/swipe_card_tv_context"
                android:layout_marginTop="@dimen/feed_screen_spacing">


                <com.jeeva.android.widgets.customfont.CustomButton
                    android:id="@+id/schedule_row_btn_vs"
                    android:layout_width="@dimen/dim_24"
                    android:layout_height="@dimen/dim_24"
                    android:gravity="center"
                    app:typeface="lato_regular"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/vs_icon" />


                <!-- NewChallengesResponse Left Image and Name -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerHorizontal="true"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/dp_16">


                    <com.jeeva.android.widgets.HmImageView
                        android:id="@+id/swipe_card_iv_left"
                        android:layout_width="@dimen/dp_30"
                        android:layout_height="@dimen/dp_30"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/dp_6"
                        android:scaleType="fitXY"
                        app:defaultImage="@drawable/left_placeholder_icon" />

                    <com.jeeva.android.widgets.customfont.CustomTextView
                        android:id="@+id/schedule_row_tv_party_a_name"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/dp_12"
                        android:layout_toRightOf="@+id/swipe_card_iv_left"
                        android:ellipsize="end"
                        android:gravity="left"
                        android:maxLines="2"
                        android:textAllCaps="false"
                        android:textColor="@color/white_80"
                        android:textSize="@dimen/dp_12"
                        app:typeface="lato_regular" />


                </RelativeLayout>

                <!-- NewChallengesResponse Right Image and Name -->
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:orientation="horizontal">

                    <com.jeeva.android.widgets.customfont.CustomTextView
                        android:id="@+id/schedule_row_tv_party_b_name"
                        android:layout_width="@dimen/dp_80"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center"
                        android:layout_marginRight="@dimen/dp_10"
                        android:layout_toLeftOf="@+id/swipe_card_iv_right"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="2"
                        android:textAllCaps="false"
                        android:textColor="@color/white_80"
                        android:textSize="@dimen/dp_12"
                        app:typeface="lato_regular" />

                    <com.jeeva.android.widgets.HmImageView
                        android:id="@+id/swipe_card_iv_right"
                        android:layout_width="@dimen/dp_30"
                        android:layout_height="@dimen/dp_30"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/dp_16"
                        android:layout_marginTop="@dimen/dp_6"
                        android:scaleType="fitXY"
                        app:defaultImage="@drawable/right_placeholder_icon" />
                </RelativeLayout>


                <com.jeeva.android.widgets.customfont.CustomButton
                    android:id="@+id/schedule_row_btn_playmatch"
                    android:layout_width="@dimen/dp_80"
                    android:layout_height="@dimen/dp_35"
                    android:layout_below="@+id/schedule_row_btn_vs"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/dp_20"
                    android:background="@drawable/play_btn_bg"
                    android:onClick="onClick"
                    android:text="PLAY"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dp_15"
                    android:visibility="gone"
                    app:typeface="lato_regular"

                    />


            </RelativeLayout>


            <View
                android:id="@+id/schedule_row_v_result_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_marginTop="@dimen/feed_screen_spacing"
                android:background="@drawable/black_divider"
                android:visibility="gone" />

            <com.jeeva.android.widgets.customfont.CustomTextView
                android:id="@+id/schedule_row_tv_match_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/dim_10"
                android:layout_marginRight="@dimen/dim_10"
                android:layout_marginTop="@dimen/dim_12"
                android:gravity="center"
                android:textColor="@color/grey6"
                android:textSize="@dimen/sp_10"
                android:visibility="visible"
                app:typeface="lato_bold" />

            <com.jeeva.android.widgets.customfont.CustomTextView
                android:id="@+id/schedule_row_tv_match_result_wait"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/dp_5"
                android:layout_marginRight="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_12"
                android:drawablePadding="@dimen/dp_8"
                android:gravity="center"
                android:textColor="@color/grey6"
                android:textSize="@dimen/sp_10"
                android:visibility="gone"
                app:typeface="lato_bold" />

        </LinearLayout>

        <!--One Party Match Layout-->
        <LinearLayout
            android:id="@+id/schedule_row_one_party_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/swipe_card_tv_context"
            android:visibility="gone"
            android:layout_marginTop="@dimen/feed_screen_spacing">

            <com.jeeva.android.widgets.customfont.CustomButton
                android:id="@+id/schedule_row_one_party_vs"
                android:layout_width="@dimen/dim_24"
                android:layout_height="@dimen/dim_24"
                android:gravity="center"
                app:typeface="lato_regular"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/vs_icon" />


            <!-- NewChallengesResponse Left Image and Name -->
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerHorizontal="true"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/dp_16">

                <com.jeeva.android.widgets.HmImageView
                    android:id="@+id/swipe_card_one_party_iv_left"
                    android:layout_width="@dimen/dp_30"
                    android:layout_height="@dimen/dp_30"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="@dimen/dp_6"
                    android:scaleType="fitXY"
                    app:defaultImage="@drawable/left_placeholder_icon" />

                <com.jeeva.android.widgets.customfont.CustomTextView
                    android:id="@+id/schedule_row_tv_one_party_a_name"
                    android:layout_width="@dimen/dp_220"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_marginTop="@dimen/dp_1"
                    android:layout_toRightOf="@+id/swipe_card_one_party_iv_left"
                    android:ellipsize="end"
                    android:gravity="left"
                    android:maxLines="2"
                    android:textAllCaps="false"
                    android:textColor="@color/white_80"
                    android:textSize="@dimen/dp_12"
                    app:typeface="lato_regular" />

                <com.jeeva.android.widgets.customfont.CustomTextView
                    android:id="@+id/schedule_row_tv_one_party_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/schedule_row_tv_one_party_a_name"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_marginTop="@dimen/dp_1"
                    android:layout_toRightOf="@+id/swipe_card_one_party_iv_left"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:textAllCaps="false"
                    android:textColor="@color/grey4"
                    android:textSize="@dimen/dp_11.5"
                    app:typeface="lato_regular" />

            </RelativeLayout>


            <com.jeeva.android.widgets.customfont.CustomTextView
                android:id="@+id/schedule_row_one_party_tv_match_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_12"
                android:gravity="left"
                android:textColor="@color/white"
                android:textSize="@dimen/dp_12"
                android:visibility="gone"
                app:typeface="lato_light" />

            <com.jeeva.android.widgets.customfont.CustomTextView
                android:id="@+id/schedule_row_one_party_tv_match_result_wait"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_12"
                android:drawablePadding="@dimen/dp_8"
                android:gravity="left"
                android:textColor="@color/white_60"
                android:textSize="@dimen/dp_13"
                android:visibility="visible"
                app:typeface="lato_regular" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/schedule_row_rl_points_summary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/match_details_with_leaderboard_layout"
            android:layout_marginLeft="@dimen/dim_10"
            android:layout_marginRight="@dimen/dim_10"
            android:padding="@dimen/dp_4">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.jeeva.android.widgets.ShadowLayout
                    android:id="@+id/schedule_row_scores_sl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/dp_4"
                    app:sl_cornerRadius="@dimen/dp_8"
                    app:sl_fillColor="@color/black"
                    app:sl_shadowRadius="@dimen/dp_4">

                    <LinearLayout
                        android:id="@+id/schedule_row_scores_ll"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dim_60"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/dim_8">

                        <RelativeLayout
                            android:id="@+id/schedule_row_rl_average_score"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/average_gradient_bg"
                            android:gravity="center">

                            <com.jeeva.android.widgets.customfont.CustomTextView
                                android:id="@+id/schedule_row_tv_average_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingLeft="@dimen/dim_4"
                                android:layout_centerHorizontal="true"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/grey_bbbbcc"
                                android:textSize="@dimen/sp_16"
                                app:typeface="lato_regular" />


                            <com.jeeva.android.widgets.customfont.CustomTextView
                                android:id="@+id/schedule_row_tv_average_score_txt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/schedule_row_tv_average_score"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="@dimen/dim_4"
                                android:gravity="center"
                                android:text="AVERAGE"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp_9"
                                app:typeface="lato_bold" />

                            <ImageView
                                android:layout_width="@dimen/dim_6"
                                android:layout_height="@dimen/dim_6"
                                android:layout_below="@+id/schedule_row_tv_average_score"
                                android:layout_marginLeft="@dimen/dim_4"
                                android:layout_marginTop="@dimen/dim_8"
                                android:layout_toRightOf="@+id/schedule_row_tv_average_score_txt"
                                android:alpha="0.6"
                                android:src="@drawable/timeline_points_arrow" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/schedule_row_rl_my_score"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/score_gradient_bg"
                            android:gravity="center">

                            <com.jeeva.android.widgets.customfont.CustomTextView
                                android:id="@+id/schedule_row_tv_my_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp_20"
                                app:typeface="lato_bold" />


                            <com.jeeva.android.widgets.customfont.CustomTextView
                                android:id="@+id/schedule_row_tv_my_score_txt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/schedule_row_tv_my_score"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="@dimen/dim_2"
                                android:gravity="center"
                                android:text="YOUR SCORE"
                                android:textColor="@color/grey_bbbbcc"
                                android:textSize="@dimen/dp_9"
                                app:typeface="lato_regular" />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/schedule_row_rl_highest_score"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/highest_gradient_bg"
                            android:gravity="center">

                            <com.jeeva.android.widgets.customfont.CustomTextView
                                android:id="@+id/schedule_row_tv_highest_score"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:gravity="center"
                                android:text="0"
                                android:paddingLeft="@dimen/dim_4"
                                android:textColor="@color/grey_bbbbcc"
                                android:textSize="@dimen/sp_16"
                                app:typeface="lato_regular" />

                            <com.jeeva.android.widgets.customfont.CustomTextView
                                android:id="@+id/schedule_row_tv_highest_score_txt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/schedule_row_tv_highest_score"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="@dimen/dim_4"
                                android:gravity="center"
                                android:text="HIGHEST"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp_9"
                                app:typeface="lato_bold" />

                            <ImageView
                                android:layout_width="@dimen/dim_6"
                                android:layout_height="@dimen/dim_6"
                                android:layout_below="@+id/schedule_row_tv_highest_score"
                                android:layout_marginLeft="@dimen/dim_4"
                                android:layout_marginTop="@dimen/dim_8"
                                android:layout_toRightOf="@+id/schedule_row_tv_highest_score_txt"
                                android:alpha="0.6"
                                android:src="@drawable/timeline_points_arrow" />


                        </RelativeLayout>

                    </LinearLayout>

                </com.jeeva.android.widgets.ShadowLayout>

                <RelativeLayout
                    android:id="@+id/schedule_row_rl_leaderboard"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dim_40"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="@dimen/dim_12"
                    android:background="@drawable/blue_border_box"
                    android:paddingBottom="@dimen/dim_12"
                    android:paddingTop="@dimen/dim_12">

                    <com.jeeva.android.widgets.customfont.CustomTextView
                        android:id="@+id/schedule_row_tv_leaderboard"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/dp_1"
                        android:gravity="bottom"
                        android:text="LEADERBOARDS"
                        android:textColor="@drawable/leaderbord_text_selector"
                        android:textSize="@dimen/dp_12"
                        app:typeface="lato_bold" />

                    <ImageView
                        android:layout_width="@dimen/dp_8"
                        android:layout_height="@dimen/dp_8"
                        android:layout_marginLeft="@dimen/dp_3"
                        android:layout_marginTop="@dimen/dp_6"
                        android:layout_toRightOf="@+id/schedule_row_tv_leaderboard"
                        android:alpha="0.6"
                        android:src="@drawable/timeline_points_arrow"
                        android:visibility="gone" />

                </RelativeLayout>

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/points_rl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/match_details_with_leaderboard_layout"
        android:layout_marginBottom="@dimen/dim_2"
        android:layout_marginTop="@dimen/dim_10">

        <com.jeeva.android.widgets.customfont.CustomTextView
            android:id="@+id/schedule_row_tv_no_of_powerups_used"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dim_3"
            android:layout_toLeftOf="@+id/schedule_row_tv_no_of_powerups_used_iv"
            android:background="@android:color/transparent"
            android:textAllCaps="false"
            android:textColor="@color/white_999999"
            android:textSize="@dimen/sp_12"
            android:visibility="visible"
            app:typeface="lato_regular" />

        <ImageView
            android:id="@+id/schedule_row_tv_no_of_powerups_used_iv"
            android:layout_width="@dimen/dim_13"
            android:layout_height="@dimen/dim_13"
            android:layout_marginRight="@dimen/dp_3"
            android:layout_marginTop="@dimen/dim_2"
            android:layout_toLeftOf="@+id/schedule_row_tv_no_of_powerups_used_txt"
            android:src="@drawable/powerup_grey_icon_small" />

        <com.jeeva.android.widgets.customfont.CustomTextView
            android:id="@+id/schedule_row_tv_no_of_powerups_used_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dim_14"
            android:background="@android:color/transparent"
            android:text="Powerups Used"
            android:textAllCaps="false"
            android:textColor="@color/white_999999"
            android:textSize="@dimen/sp_12"
            android:visibility="visible"
            app:typeface="lato_regular" />

        <com.jeeva.android.widgets.customfont.CustomTextView
            android:id="@+id/schedule_row_tv_match_correct_questions"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dim_14"
            android:layout_marginRight="@dimen/dim_14"
            android:gravity="left"
            android:maxLines="2"
            android:textColor="@color/white_999999"
            android:textSize="@dimen/sp_12"
            android:visibility="visible"
            app:typeface="lato_regular" />


    </RelativeLayout>

    <LinearLayout
        android:id="@+id/my_results_row_ll_predictions"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/points_rl"
        android:layout_marginBottom="@dimen/dp_2"
        android:layout_marginTop="@dimen/dp_2"
        android:orientation="vertical"
        android:showDividers="beginning">

    </LinearLayout>


    <LinearLayout
        android:id="@+id/my_results_row_ll_leaderboardbtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/my_results_row_ll_predictions"
        android:layout_marginBottom="@dimen/dp_2"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"
        android:orientation="vertical"
        android:showDividers="beginning" />


    <LinearLayout
        android:id="@+id/schedule_row_ll_match_commentary_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/match_details_with_leaderboard_layout"
        android:orientation="vertical"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/my_results_nostra_logo"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_40"
        android:layout_below="@id/my_results_row_ll_leaderboardbtn"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/dp_10"
        android:src="@drawable/sportscafe_logo"
