<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/black">



    <com.jeeva.android.widgets.customfont.CustomTextView
        android:id="@+id/feed_row_tv_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:layout_marginLeft="@dimen/dp_15"
        android:padding="@dimen/dp_5"
        android:text="Date"
        android:background="@color/black"
        android:textColor="@color/pink_swan"
        android:textSize="@dimen/dp_16"
        app:typeface="roboto_condensed"

        />


    <android.support.v4.widget.Space
        android:id="@+id/feed_row_space"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:layout_below="@+id/feed_row_tv_date"
        />

    <View
        android:id="@+id/view"
        android:layout_width="@dimen/dp_1"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="@dimen/dp_50"
        android:layout_marginTop="@dimen/dp_33"
        android:layout_alignBottom="@id/feed_row_space"
        android:background="@color/dividercolor"
        />


    <LinearLayout
        android:id="@+id/feed_row_ll_tour_parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/feed_row_space"
        >

    </LinearLayout>


</RelativeLayout>